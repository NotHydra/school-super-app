<aside class="main-sidebar sidebar-dark-primary elevation-4">
    <a href="/" class="brand-link">
        <img src="dist/img/rpl-logo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: 0.8" />
        <span class="brand-text font-weight-light">School Super App</span>
    </a>

    <div class="sidebar">
        <div class="user-panel mt-3 pb-3 mb-3 d-flex">
            <div class="image">
                <img src="dist/img/default-user.jpg" class="img-circle elevation-2" alt="User Image" />
            </div>
            <div class="info">
                <div class="d-block">Username</div>
            </div>
        </div>

        <nav class="mt-2">
            <ul class="nav nav-pills nav-sidebar flex-column nav-child-indent" data-widget="treeview" role="menu" data-accordion="false">
                <% pageItemArray.forEach((pageItemObject, pageItemIndex) => { %>
                    <li class="nav-item <%= pageItemIndex == navActive[0] ? "menu-open" : "" %>">
                        <a class="nav-link <%= pageItemIndex == navActive[0] ? "active" : "" %>">
                            <i class="nav-icon fas fa-<%= pageItemObject.icon %>"></i>
                            <p>
                                <%= pageItemObject.title %>
                                <i class="right fas fa-angle-left"></i>
                            </p>
                        </a>

                        <ul class="nav nav-treeview">
                            <% pageItemObject.child.forEach((pageItemChildObject, pageItemChildIndex) => { %>
                                <li class="nav-item">
                                    <a href="/<%= pageItemChildObject.link %>" class="nav-link <%= pageItemIndex == navActive[0] && pageItemChildIndex == navActive[1] ? "active" : "" %>">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p><%= pageItemChildObject.title %></p>
                                    </a>
                                </li>
                            <% }); %>
                        </ul>
                    </li>
                <% }); %>
            </ul>
        </nav>
    </div>
</aside>
