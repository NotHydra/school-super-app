<% const navItemObject = navItemArray[navActive[0]]; const navItemChildObject = navItemObject.child[navActive[1]] %>

<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0"><%= navItemObject.title %> - <%= navItemChildObject.title %></h1>
            </div>

            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="<%= navItemChildObject.link %>"><%= navItemChildObject.title %></a>
                    </li>
                    <li class="breadcrumb-item active"><%= navItemObject.title %></li>
                </ol>
            </div>
        </div>
    </div>
</div>
